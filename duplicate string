#include<stdio.h>
#include<string.h>
void main()
{
   char string[100];
   int count;
   printf("Enter the string\n");
   scanf("%s",string);
   printf("Duplicate string\n");
   for(int i=0;i<strlen(string);i++)
   {
      count=1;
      for(int j=i+1;j<srlen(string);j++)
      {
          if(string[i]==string[j] && string[i]!=' ')
          {
              count++;
              string[j]='0';
           }
       }
       if(count >1 && string[i]!='0')
       printf("%c",string[i]);
       
    }
 }
